<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.gintongame.mapping.PerWorkInfoMapper">

    <insert id="insertPerWorkInfo" parameterType="JavaBean.PerWorkInfo" useGeneratedKeys="true">
        insert into per_work_info(uuid,oname,department,job,level,job_early,work_time,wtype,company_addr,oph,income,w_desc) values(
      #{uuid},#{oname},#{department},#{job},#{level},#{job_early},#{work_time},#{wtype},#{company_addr},#{oph},#{income},#{w_desc}
        );
    </insert>

</mapper>