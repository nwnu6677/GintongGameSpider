            <gtype></gtype>
        </gtypes>
        <!--分类-->
        <sort>//ul[@id='detail_line_ul']/li[1]/text()</sort>
        <!--时间-->
        <web_update_time formatStr="yyyy年MM月dd">//ul[@id='detail_line_ul']/li[3]/text()</web_update_time>
        <!--大小-->
        <game_size>//ul[@id='detail_line_ul']/li[4]/span/text()</game_size>
        <!--系统-->
        <platform>//ul[@id='detail_line_ul']/li[5]/text()</platform>
        <!--资费-->
        <charge_mode>//ul[@id='detail_line_ul']/li[6]/span/text()</charge_mode>
        <!--游戏状态(研发进度)-->
        <dpprogress></dpprogress>
        <!--画面风格（游戏风格）-->
        <gstyle></gstyle>
        <!--画面方式-->
        <gamespy></gamespy>
        <!--开服总数-->
        <totalServer></totalServer>
        <!--游戏题材-->
        <gtheme></gtheme>
        <!--作者-->
        <develop_com>//ul[@id='detail_line_ul']/li[7]/text()</develop_com>
        <!--介绍-->
        <g_desc>//div[@class='app_detail_infor']/p/text()</g_desc>
        <!--截图urls-->
        <picture>//div[@class='section-body']/div/ul/li/img/@src</picture>
        <!--起始页面-->
        <urls>
            <url>
                http://www.anzhi.com/sort_21_1_hot.html
            </url>
            <url>
                http://www.anzhi.com/sort_14_1_hot.html
            </url>
            <url>
                http://www.anzhi.com/sort_15_1_hot.html
            </url>
            <url>
                http://www.anzhi.com/sort_16_1_hot.html
            </url>
            <url>
                http://www.anzhi.com/sort_19_1_hot.html
            </url>
            <url>
                http://www.anzhi.com/sort_20_1_hot.html
            </url>
            <url>
                http://www.anzhi.com/sort_24_1_hot.html
            </url>
            <url>
                http://www.anzhi.com/sort_56_1_hot.html
            </url>
            <url>
                http://www.anzhi.com/sort_57_1_hot.html
            </url>
        </urls>
        <!--网站-->
        <source>安智市场</source>

        <!--详情页url-->
        <detailurl>//span[@class='app_name']/a/@href</detailurl>
        <!--下一页-->
        <nextpage>//a[@class='next']/@href</nextpage>
        <!--游戏英语名字-->
        <gename></gename>
        <!--语言-->
        <language></language>
        <!--发行商-->
        <publisher></publisher>
        <!--发行区域-->
        <issue_area></issue_area>
        <!--标签-->
        <gtags></gtags>
        <!--游戏视角-->
        <viewpoint></viewpoint>
        <!--游戏引擎-->
        <engine></engine>
        <!--游戏品级-->
        <grade></grade>
        <!--产品团队规模-->
        <scale></scale>
        <!--产品需求-->
        <pdemand></pdemand>
        <!--价格-->
        <price></price>
        <!--初版出售时间-->
        <ptime formatStr=""></ptime>
        <!--公测时间-->
        <beta_time formatStr=""></beta_time>
        <!--内测时间-->
        <test_time formatStr=""></test_time>
        <!--封测时间-->
        <betatest_time formatStr=""></betatest_time>
        <!--立项时间-->
        <set_time formatStr=""></set_time>
        <!--游戏官网-->
        <web></web>
        <!--运营商-->
        <operator></operator>
        <!--资料片发布时间-->
        <films_time></films_time>
        <!--下载链接-->
        <download_link></download_link>
